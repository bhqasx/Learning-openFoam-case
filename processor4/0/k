FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 3e-05;

boundaryField
{
    top
    {
        type            symmetryPlane;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 3e-05;
    }
    outlet
    {
        type            zeroGradient;
    }
    outletConnectMaster
    {
        type            kqRWallFunction;
        value           nonuniform 0();
    }
    walls
    {
        type            kqRWallFunction;
        value           uniform 3e-05;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 3e-05;
    }
}


